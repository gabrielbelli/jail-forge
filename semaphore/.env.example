# Example environment variables
# Copy this file to .env and customize for your environment
# Consider using ansible-vault for sensitive values

# BSD Host Configuration
BSD_HOST_IP=192.168.1.10
BSD_HOST_USER=root
JAIL_INTERFACE=em0

# Network Configuration
JAIL_NETWORK=192.168.1.0/24
JAIL_GATEWAY=192.168.1.1
SEMAPHORE_DB_IP=192.168.1.50
SEMAPHORE_APP_IP=192.168.1.51

# Database Configuration
POSTGRES_VERSION=15
POSTGRES_DB_NAME=semaphore
POSTGRES_DB_USER=semaphore
POSTGRES_DB_PASSWORD=changeme_strong_password

# Semaphore Configuration
SEMAPHORE_VERSION=v2.9.0
SEMAPHORE_PORT=3000
SEMAPHORE_ADMIN_USER=admin
SEMAPHORE_ADMIN_EMAIL=admin@example.com
SEMAPHORE_ADMIN_PASSWORD=changeme_strong_password

# Secrets (generate random values for production)
SEMAPHORE_COOKIE_HASH=$(openssl rand -hex 32)
SEMAPHORE_COOKIE_ENCRYPTION=$(openssl rand -hex 32)
SEMAPHORE_ACCESS_KEY_ENCRYPTION=$(openssl rand -hex 32)

# ZFS Configuration
ZFS_POOL=zroot
JAIL_DATASET=jails

# FreeBSD Version
FREEBSD_VERSION=13.2-RELEASE
